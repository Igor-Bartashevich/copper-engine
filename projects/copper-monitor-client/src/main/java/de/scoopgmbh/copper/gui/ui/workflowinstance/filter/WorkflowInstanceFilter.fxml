<FlowPane alignment="CENTER" hgap="5.0" layoutX="0.0" layoutY="0.0" prefHeight="40.0" prefWidth="811.0" xmlns:fx="http://javafx.com/fxml">
  <children>
    <Label text="state">
      <labelFor>
        <TextField id="workflowClass" fx:id="priorityField" prefWidth="200.0" />
      </labelFor>
    </Label>
    <ChoiceBox fx:id="stateChoice">
      <items>
        <FXCollections fx:factory="observableArrayList">
          <String fx:value="Item 1" />
          <String fx:value="Item 2" />
          <String fx:value="Item 3" />
        </FXCollections>
      </items>
    </ChoiceBox>
    <Label text="priority">
      <labelFor>
        <TextField fx:id="minorVersion" prefWidth="139.0" />
      </labelFor>
    </Label>
    <fx:reference source="priorityField" />
  </children>
</FlowPane>
