<BorderPane layoutX="0.0" layoutY="0.0" prefHeight="460.0" prefWidth="616.0" xmlns:fx="http://javafx.com/fxml">
  <bottom>
      <HBox prefHeight="37.0" prefWidth="616.0">
          <Button fx:id="reset" mnemonicParsing="false" text="reset">
              <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
              </HBox.margin>
          </Button>
      </HBox>
  </bottom>
  <center>
    <BarChart fx:id="chart">
      <xAxis>
        <CategoryAxis side="BOTTOM">
          <categories>
            <FXCollections fx:factory="observableArrayList" />
          </categories>
        </CategoryAxis>
      </xAxis>
      <yAxis>
        <NumberAxis side="LEFT" />
      </yAxis>
    </BarChart>
  </center>
</BorderPane>
